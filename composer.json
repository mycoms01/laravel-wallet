{
    "name": "Mycoms01/laravel-wallet",
    "description": "Easy to use virtual wallet for your app",
    "keywords": [
        "depsimon",
        "laravel-wallet",
        "virtual",
        "currency",
        "credits",
        "wallet"
    ],
    "homepage": "",
    "license": "MIT",
    "authors": [{
        "name": "",
        "email": ""
    }],
    "require": {
        "php": "^7.0",
        "illuminate/database": "~5.6"
    },
    "autoload": {
        "psr-4": {
            "Depsimon\\Wallet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Depsimon\\Wallet\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Depsimon\\Wallet\\WalletServiceProvider"
            ],
            "aliases": {
                "Wallet": "Depsimon\\Wallet\\WalletFacade"
            }
        }
    }

}